{
  "name": "@arethetypeswrong/core",
  "version": "0.0.7",
  "description": "The API behind arethetypeswrong.github.io",
  "author": "Andrew Branch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arethetypeswrong/arethetypeswrong.github.io.git",
    "directory": "packages/core"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "pretest": "tsc -b test",
    "test": "node --test test/dist",
    "snapshot": "node scripts/createSnapshotFixture.js",
    "patch": "npm version patch && npm run commit-patch",
    "commit-patch": "git commit -am \"Bump core to v$npm_package_version\" && git tag \"v$npm_package_version\"",
    "prepublishOnly": "npm run tsc && npm run test"
  },
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./problems": {
      "development": "./src/problems.ts",
      "default": "./dist/problems.js"
    },
    "./types": {
      "development": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./utils": {
      "development": "./src/utils.ts",
      "default": "./dist/utils.js"
    },
    "./versions": {
      "node": "./dist/versions.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@andrewbranch/untar.js": "^1.0.0",
    "fetch-ponyfill": "^7.1.0",
    "fflate": "^0.7.4",
    "typescript": "^5.0.0-dev.20230207"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/ts-expose-internals": "npm:ts-expose-internals@4.9.4"
  },
  "volta": {
    "node": "19.8.1"
  }
}
